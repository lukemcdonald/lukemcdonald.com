{
  "name": "lukemcdonald-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check",
    "clean": "rm -rf .astro dist node_modules",
    "deploy:prod": "netlify deploy --build --prod",
    "dev": "astro dev",
    "dev:netlify": "netlify dev",
    "fix": "npm run lint:fix && npm run format:fix",
    "format": "prettier .",
    "format:check": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write --ignore-unknown",
    "lint": "eslint ./src --report-unused-disable-directives",
    "lint:fix": "eslint ./src --fix --report-unused-disable-directives",
    "preview": "netlify deploy --build --message \"preview $(git rev-parse --short HEAD)\"",
    "reinstall": "npm run clean && npm ci",
    "style": "npm run lint:fix && npm run format:fix",
    "validate": "npm run -s lint && npm run -s format:check && npm run -s check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.5.1",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@headlessui/react": "^2.2.6",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.12.0",
    "date-fns": "4.1.0",
    "lucide-astro": "^0.539.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript-eslint": "^8.37.0"
  }
}
