{
  "name": "lukemcdonald-astro",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check",
    "clean": "rm -rf .astro dist node_modules",
    "deploy:prod": "netlify deploy --build --prod",
    "dev": "astro dev",
    "dev:netlify": "netlify dev",
    "fix": "pnpm run lint:fix && pnpm run format:fix",
    "format": "prettier .",
    "format:check": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write --ignore-unknown",
    "lint": "eslint ./src --report-unused-disable-directives",
    "lint:fix": "eslint ./src --fix --report-unused-disable-directives",
    "preinstall": "npx only-allow pnpm",
    "preview": "netlify deploy --build --message \"preview $(git rev-parse --short HEAD)\"",
    "reinstall": "pnpm run clean && pnpm install --frozen-lockfile",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run -s lint && pnpm run -s format:check && pnpm run -s check"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "date-fns": "4.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/netlify": "^6.6.2",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.0",
    "@lucide/astro": "^0.544.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.0",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.43.0"
  }
}
