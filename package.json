{
  "name": "lukemcdonald-astro",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check",
    "clean": "rm -rf .astro dist node_modules",
    "deploy:prod": "netlify deploy --build --prod",
    "dev": "astro dev",
    "dev:netlify": "netlify dev",
    "fix": "pnpm run lint:fix && pnpm run format:fix",
    "format": "biome format .",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false --assist-enabled=false .",
    "format:fix": "biome format --write .",
    "lint": "eslint ./src --report-unused-disable-directives",
    "lint:fix": "eslint ./src --fix --report-unused-disable-directives",
    "preinstall": "npx only-allow pnpm",
    "preview": "netlify deploy --build --message \"preview $(git rev-parse --short HEAD)\"",
    "reinstall": "pnpm run clean && pnpm install --frozen-lockfile",
    "validate": "pnpm run -s lint && pnpm run -s format:check && pnpm run -s check"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "date-fns": "4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.5.8",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.1",
    "@lucide/astro": "^0.542.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "astro": "^5.13.3",
    "eslint": "^9.34.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}
